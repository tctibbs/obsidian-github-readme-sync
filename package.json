{
	"name": "obsidian-github-readme-sync",
	"version": "0.1.0",
	"description": "Obsidian plugin for syncing GitHub repositories' Markdown files to your vault",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"github",
		"sync",
		"markdown",
		"readme"
	],
	"author": "Your Name",
	"license": "MIT",
	"devDependencies": {
		"@types/micromatch": "^4.0.9",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"micromatch": "^4.0.5"
	}
}
